{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Remote Delve",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "/app",   // Path to your Go project on the remote machine
            "port": 2345,           // Port Delve is listening on
            "host": "127.0.0.1",    // IP address of the remote machine; use "localhost" if running locally
            "apiVersion": 2,
        }
    ]
}
